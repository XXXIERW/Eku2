.ad1 {
    margin-top: 50rpx;
}

.ad1_close {
    width: 100rpx;
    height: 50rpx;
    background: #0f0;
    z-index: 5;
    position: absolute;
    right: 50%;
    top: 50rpx;
    visibility: hidden;
}

.ad {
    width: 100%;
    height: 200rpx;
}

.content {
    position: fixed;
    left: 0;
    top: 0;
    bottom: 0;
    right: 0;
    background: #000;
}

.popBox {
    position: fixed;
    left: 0;
    right: 0;
    bottom: -400rpx;
    top: 0;
}

.pop-image {
    position: absolute;
    margin: 0 auto;
    bottom: 0;
    left: 0;
    right: 0;
}

.popBox image {
    transform: rotate(90deg);
    width: 100%;
    height: 100%;
    margin: 0 auto;
}

.popCont {
    width: 100%;
    height: 100%;
    position: relative;
    display: flex;
    justify-content: center;
}

.popTxt {
    padding-left: 400rpx;
    color: #fff;
    font-size: 160rpx;
    display: inline-block;
    transform: rotate(90deg) translateY(-50%);
    position: absolute;
    bottom: 0;
    height: 380rpx;
    white-space: nowrap;
    letter-spacing: 30rpx;
}

@-webkit-keyframes first {
    from {
        top: 3000rpx;
    }

    to {
        top: -100%;
    }
}

@keyframes first {
    from {
        top: 3000rpx;
    }

    to {
        top: -100%;
    }
}

.sendForm {
    position: fixed;
    left: 0;
    right: 0;
    bottom: 0;
    display: flex;
    justify-content: center;
    align-items: center;
    padding-top: 5rpx;
    padding-bottom: 5rpx;
    z-index: 2;
