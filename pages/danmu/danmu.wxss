@-webkit-keyframes flash {
    0% {
        background: aqua;
    }

    10% {
        background: yellow;
    }

    20% {
        background: fuchsia;
    }

    30% {
        background: blue;
    }

    40% {
        background: green;
    }

    50% {
        background: lime;
    }

    60% {
        background: #00ffff;
    }

    70% {
        background: navy;
    }

    80% {
        background: red;
    }

    90% {
        background: teal;
    }

    100% {
        background: white;
    }
}

