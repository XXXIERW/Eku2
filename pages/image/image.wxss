page {
    background: url(data:image/png;base64,) ;}

.background {
    position: fixed;
    left: 0;
    top: 0;
    bottom: 0;
    right: 0;
    background: url(data:image/png;base64,) ;
}

.item-image image {
    transform: rotate(90deg);
}

.layout-image {
    position: fixed;
}

.layout-setting {
    position: fixed;
    right: -1000rpx;
    top: 500rpx;
    z-index: 3;
}

.layout-setting button {
    margin-top: 10rpx;
    color: #fff;
    font-size: 30rpx;
    border-top-left-radius: 40rpx;
    border-bottom-left-radius: 40rpx;
    border-top-right-radius: 0;
    border-bottom-right-radius: 0;
    padding-left: 30rpx;
    padding-top: 0rpx;
    padding-bottom: 0rpx;
    background: #333333;
}

.scorll-view {
    width: 100%;
    height: 250rpx;
    white-space: nowrap;
}

.setting-image {
    width: 250rpx;
    height: 250rpx;
    margin-right: 20rpx;
}

.bottom-setting {
    position: fixed;
    bottom: 0;
    background: black;
    width: 100%;
}

.bottom-setting button {
    font-size: 30rpx;
    width: 200rpx;
    margin: 10rpx;
}

.tips {
    color: #fff;
    font-size: 30rpx;
    margin: 10rpx;
}

.item-setting-image {
    width: 250rpx;
    height: 100rpx;
    display: inline-block;
}

.image-close {
    position: relative;
    width: 45rpx;
    height: 45rpx;
    right: -200rpx;
    top: -250rpx;
    background: #f00;
    border-radius: 50%;
}

.image-close image {
    width: 30rpx;
    height: 30rpx;
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    margin: auto;
}

.ad {
    width: 100%;
    height: 200rpx;
}